<?xml version="1.0" encoding="utf-8"?>
<GuiSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <DoubleClickInterval>0.35</DoubleClickInterval>
  <Skinners>
    <Controller>Label</Controller>
    <Skinner>LabelDrawer</Skinner>
  </Skinners>
  <Skinners>
    <Controller>Image</Controller>
    <Skinner>ImageDrawer</Skinner>
  </Skinners>
  <Skinners>
    <Controller>Window</Controller>
    <Skinner>WindowDrawer</Skinner>
    <Settings xsi:type="WindowDrawerSettings">
      <BackgroundColor>
        <X>1</X>
        <Y>1</Y>
        <Z>1</Z>
        <W>1</W>
      </BackgroundColor>
      <BorderColor>
        <X>1</X>
        <Y>1</Y>
        <Z>1</Z>
        <W>1</W>
      </BorderColor>
    </Settings>
  </Skinners>
  <Skinners>
    <Controller>Button</Controller>
    <Skinner>ButtonDrawer</Skinner>
    <Settings xsi:type="ButtonDrawerSettings">
      <BackgroundColor>
        <X>0.2</X>
        <Y>0.3</Y>
        <Z>0.2</Z>
        <W>1</W>
      </BackgroundColor>
      <BorderColor>
        <X>0.1</X>
        <Y>0.1</Y>
        <Z>0.1</Z>
        <W>1</W>
      </BorderColor>
      <HoverColor>
        <X>0.6</X>
        <Y>0.6</Y>
        <Z>0.3</Z>
        <W>1</W>
      </HoverColor>
      <CornerRoundness>5</CornerRoundness>
      <CornerSlices>5</CornerSlices>
      <FadeAmount>0.05</FadeAmount>
    </Settings>
  </Skinners>
  <DefaultFont>Courier</DefaultFont>
  <DefaultFontSize>8</DefaultFontSize>
  <MouseSkin>default</MouseSkin>
  <SkinColor>
    <X>1</X>
    <Y>1</Y>
    <Z>1</Z>
    <W>1</W>
  </SkinColor>
  <Shaders>
    <Programs>
      <ShaderProgram Name="gui.button">
        <Shaders>
          <Shader>
            <Data>gui/draw.vert</Data>
            <Inline>false</Inline>
            <Type>VertexShader</Type>
          </Shader>
          <Shader>
            <Data>gui/button.frag</Data>
            <Inline>false</Inline>
            <Type>FragmentShader</Type>
          </Shader>
        </Shaders>
        <Uniforms>
          <Uniform>mP</Uniform>
          <Uniform>mM</Uniform>
          <Uniform>color</Uniform>
          <Uniform>fade</Uniform>
        </Uniforms>
      </ShaderProgram>
      <ShaderProgram Name="gui.texture">
        <Shaders>
          <Shader>
            <Data>gui/draw.vert</Data>
            <Inline>false</Inline>
            <Type>VertexShader</Type>
          </Shader>
          <Shader>
            <Data>gui/draw.frag</Data>
            <Inline>false</Inline>
            <Type>FragmentShader</Type>
          </Shader>
        </Shaders>
        <Uniforms>
          <Uniform>mP</Uniform>
          <Uniform>mM</Uniform>
        </Uniforms>
      </ShaderProgram>
    </Programs>
  </Shaders>
</GuiSettings>